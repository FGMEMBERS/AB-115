<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>rpm-gauge.ac</path>



 <animation>
  <type>scale</type>
  <x-offset>0.8672756</x-offset>
  <y-offset>0.8672756</y-offset>
  <z-offset>0.8672756</z-offset>
 </animation>
 

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
            <emission>
            <red>0.028</red>
            <green>0.014</green>
            <blue>0.007</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/engines/engine/rpm</property>
    <interpolation>
      <entry><ind>    0</ind><dep>   0 </dep></entry>
      <entry><ind>  0</ind><dep>  0 </dep></entry>
      <entry><ind> 500</ind><dep>  37 </dep></entry>
      <entry><ind> 1000</ind><dep> 75 </dep></entry>
      <entry><ind> 1500</ind><dep> 112 </dep></entry>
      <entry><ind> 2000</ind><dep> 149 </dep></entry>
      <entry><ind> 2500</ind><dep> 186 </dep></entry>
      <entry><ind> 4500</ind><dep> 334 </dep></entry>
      <entry><ind> 5000</ind><dep> 334 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
  </animation>

</PropertyList>
